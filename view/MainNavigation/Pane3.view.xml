<mvc:View controllerName="DemoUXapp.controller.MainNavigation.Pane3"
    height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">

    <List id="standardlist"
        updateFinished="onUpdateFinished"
        items="{path:'/'}">
        <headerToolbar>
            <Toolbar>
                <Title id="title" />
            </Toolbar>
        </headerToolbar>
        <items>
            <StandardListItem title="{Name}"
                iconDensityAware="false"
                iconInset="false"
                type="Navigation"
                press="onPress"
                info="{ProductId}" />
        </items>
    </List>

</mvc:View>